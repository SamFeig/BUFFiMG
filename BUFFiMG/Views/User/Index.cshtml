@model UserImagesModel
@{
    ViewData["Title"] = "User";
}

<body>
<div class="text-center">
    <h1 class="display-4" style="color: white">Your uploaded images:</h1>
</div>
<div align="center" style="margin: 10%">
    @foreach (var image in Model.imageList)
    {
    <div class="row">
        <div class="col"/>
        <div class="col-12">
            <img src=@image.src class="img-fluid"/>
        </div>
        <div class="col"/>
    </div>
    }
</div>
</body>